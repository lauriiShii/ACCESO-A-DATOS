import java.sql.SQLException;

public class Ejercicio12 {

	/**Mostrar el horario de un curso. Añadir un asterisco en aquellos tramos horarios donde haya
desdoble.**/
	
	public static void main(String[] args) throws SQLException {
		
        //ABRIR CONEXION
        LibreriaJDBC.abrirConexion();

        //MOSTRAMOS CONTENIDO
        LibreriaJDBC.dameHorario("DAM", "1A");
		
        //CERRAR CONEXION
        LibreriaJDBC.cerrarConexion();

	}

}
